<%@ val leagueName: String %>
<%@ val leagueURI: String %>
<%
   import org.mystic.h2h.fantasy.PlayersCrawler
   val count = PlayersCrawler.playersCount(leagueURI)
   val players = PlayersCrawler.getAllPlayers(leagueURI)
%>

<h2>Fantasy ${leagueName}</h2>

<h4>Игроки</h4>
<ul>
#for (i <- 0 until count)
  <li>${players(i)}</li>
#end
</ul>

