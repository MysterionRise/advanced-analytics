<%@ val leagueName: String %>
<%@ val leagueURI: String %>
<%
   import org.mystic.h2h.fantasy.PlayersCrawler
   val players = PlayersCrawler.getAllTeamsInLeague(leagueURI)
   val count = players.length
%>

<h2>Fantasy ${leagueName}</h2>

<h4>Игроки (${count})</h4>
<ul>
#for (i <- 0  until count)
  <li>
  <div>Игрок: <a href=${players(i)._3}>${players(i)._4}</a><br>
  Команда: <a href=${players(i)._1}>${players(i)._2}</a><br>
  Количество очков: ${players(i)._5}<br></div>
  </li>
#end
</ul>



